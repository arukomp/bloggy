{
    "name": "arukomp/bloggy",
    "description": "A plug-in Blog component for your Laravel project",
    "type": "library",
    "license": "MIT",
    "authors": [
        {
            "name": "Arunas Skirius",
            "email": "arukomp@gmail.com"
        }
    ],
    "minimum-stability": "dev",
    "require": {
        "php": ">=7.1.3",
        "laravel/framework": "5.6.*"
    },
    "require-dev": {
        "orchestra/testbench": "~3.0",
        "fzaninotto/faker": "~1.4",
        "phpunit/phpunit": "~7.0"
    },
    "autoload": {
        "psr-4": {
            "Arukomp\\Bloggy\\": "src/"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "Arukomp\\Bloggy\\Tests\\": "tests/"
        }
    },
    "extra": {
        "laravel": {
            "providers": [
                "Arukomp\\Bloggy\\BloggyServiceProvider",
                "Arukomp\\Bloggy\\Providers\\ViewComposerServiceProvider"
            ],
            "aliases": {}
        }
    }
}
