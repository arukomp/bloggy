{
    "name": "arukomp/bloggy",
    "description": "A plug-in Blog component for your Laravel project",
    "type": "project",
    "authors": [
        {
            "name": "Arunas Skirius",
            "email": "arukomp@gmail.com"
        }
    ],
    "minimum-stability": "dev",
    "require": {
        "php": ">=7.1.3",
        "laravel/framework": "5.6.*",
        "laravel/tinker": "~1.0",
        "predis/predis": "^1.1"
    },
    "require-dev": {
        "filp/whoops": "~2.0",
        "fzaninotto/faker": "~1.4",
        "mockery/mockery": "~1.0",
        "phpunit/phpunit": "~7.0"
    },
    "autoload": {
        "psr-4": {
            "Arukomp\\Bloggy\\": "src/"
        }
    },
    "extra": {
        "laravel": {
            "providers": [
                "Arukomp\\Bloggy\\BloggyServiceProvider",
                "Arukomp\\Bloggy\\Providers\\ComposerServiceProvider"
            ],
            "aliases": {}
        }
    },
    "scripts": {
        "post-update-cmd": [
            "php artisan vendor:publish --tag=bloggy_config --force",
            "php artisan vendor:publish --tag=bloggy_assets --force"
        ]
    }
}
